{
  "name": "sortable-table",
  "homepage": "https://github.com/stevenrskelton/sortable-table",
  "description": "Polymer Web Component that generates a sortable <table> from inlined or AJAX JSON, JSON5, and arrays.",
  "main": "sortable-table.html",
  "authors": [
    "Steven Skelton"
  ],
  "license": "MIT",
  "ignore": [
    "**/.*",
    "node_modules",
    "bower_components",
    "test",
    "tests"
  ],
  "keywords": [
    "web-component",
    "web-components",
    "polymer",
    "table",
    "grid",
    "paging",
    "sort",
    "list",
    "json5",
    "datatable",
    "datatables",
    "ajax"
  ],
  "dependencies": {
    "json5": "json5#>=0.4.0",
    "time-elements": "time-elements#>=0.4.0",
    "polymer": "Polymer/polymer#^0.5.5",
    "core-ajax": "Polymer/core-ajax#^0.5.5",
    "core-dropdown": "Polymer/core-dropdown#^0.5.5"
  },
  "devDependencies": {
    "web-component-tester": "Polymer/web-component-tester#>=2.2.6",
    "bootstrap": "bootstrap#>=3.3.1",
    "polymer-jsonp": "polymer/polymer-jsonp#>=0.3.0",
    "core-elements": "Polymer/core-elements#^0.5.5",
    "core-scroll-threshold": "Polymer/core-scroll-threshold#^0.5.5",
    "paper-item": "Polymer/paper-item#^0.5.5"
  }
